@model First_Exam.Data.Entities.Ticket

@{
    ViewData["Title"] = "EditCheckTicket";
}

<div class="CheckTicket">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-body justify-content-center text-center ">
                 <div class="panel-heading">
                    <h3 class="panel-title mt-auto justify-content-center" style="font-size:200%; color:black;">El boleto no ha sido usado</h3>
                </div>
                <div class="col-md-10 offset-4">
                    <a asp-action="EditTicket" asp-route-id="@Model?.Id" class="link-login3">Registrar</a>
                    <a asp-action="CheckTicket" class="link-login3">Regresar</a>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @{
    await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }

}